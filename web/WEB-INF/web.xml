<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>indexServletTaskReport</servlet-name>
        <servlet-class>ext.by.peleng.reports.taskReportWEB.indexServletTaskReport</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>indexServletTaskReport</servlet-name>
        <url-pattern>/servlet/taskReportWEB/index</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>usersServletTaskReport</servlet-name>
        <servlet-class>ext.by.peleng.reports.taskReportWEB.usersServletTaskReport</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>usersServletTaskReport</servlet-name>
        <url-pattern>/servlet/taskReportWEB/users</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>taskReportServlet</servlet-name>
        <servlet-class>ext.by.peleng.reports.taskReportWEB.taskReportServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>taskReportServlet</servlet-name>
        <url-pattern>/servlet/taskReportWEB/taskReport</url-pattern>
    </servlet-mapping>

</web-app>